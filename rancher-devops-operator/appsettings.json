{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "KubeOps.Operator.Watcher.ResourceWatcher": "Warning",
      "ZiggyCreatures.Caching.Fusion.FusionCache": "Warning"
    }
  },
  "Rancher": {
    "Url": "https://rancher.local",
    "Token": "",
    "AllowInsecureSsl": false,
    "CleanupNamespaces": false,
    "ObserveMethod": "watch",
    "ClusterCheckInterval": 5,
    "PollingInterval": 2
  }
}
